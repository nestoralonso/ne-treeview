{
  "name": "ne-treeview",
  "version": "2.0.0",
  "description": "displays a tree like control, must me used with bootstrap 3",
  "files": [
    "*.md",
    "docs",
    "es6",
    "lib",
    "umd"
  ],
  "homepage": "https://github.com/nestoralonso/ne-treeview#readme",
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es",
    "lint": "eslint --fix --ignore-path=.gitignore .",
    "build-umd": "cross-env BABEL_ENV=development webpack",
    "build-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel ./src/ -d lib --ignore '__tests__'",
    "build-es": "rimraf es6 && cross-env BABEL_ENV=es babel ./src/ -d es6 --ignore '__tests__'",
    "test:watch": "jest src --watch",
    "test": "cross-env CI=true jest src"
  },
  "author": "Nestor Alonso",
  "bugs": "https://github.com/nestoralonso/ne-treeview/issues",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nestoralonso/ne-treeview.git"
  },
  "devDependencies": {
    "@webpack-cli/migrate": "^0.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.14.0",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "cross-env": "^2.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-react": "^6.2.0",
    "express": "^4.14.0",
    "jest": "^15.1.1",
    "react": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-test-renderer": "^15.3.1",
    "rimraf": "^2.5.4",
    "webpack": "^2.7.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "engines": {
    "node": ">=4"
  }
}
